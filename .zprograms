###      PROGRAMS     ###

function extraer ()
{
  if [ -f "$1" ] ; then
    case "$1" in
      *.tar)                tar xf $1        ;;
      *.tar.bz2 | *.tbz2 )  tar xjvf $1        ;;
      *.tar.gz | *.tgz )    tar xzvf $1     ;;
      *.bz2)                bunzip2 $1       ;;
      *.rar)                unrar x $1     ;;
      *.gz)                 gunzip $1     ;;
      *.zip)                unzip $1     ;;
      *.7z)                 7z x $1    ;;
      *.ace)                 unace x $1    ;;
      *)   echo ""${1}" no se reconoce el formato.()" ;;
     esac
   else
     echo ""${1}" No es un archivo valido"
   fi
}

