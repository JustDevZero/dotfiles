install:
	cp .zshrc ~/
	cp .emacs ~/
	cp -r .emacs.d
	cp .zshort ~/
	cp .zcompletion ~/
	cp .gitconfig  ~/
	cp .gittemplate ~/
	cp -r .zsh ~/
	cp .vimrc ~/
	cp .profile ~/
	cp .zshrc /root
	cp .zshort /root
	cp .zcompletion/root
	cp .gitconfig  /root
	cp .gittemplate /root
	cp -r .zsh /root
	cp .vimrc /root
	cp .profile /root

